{
  "compilerOptions": {
    "module": "CommonJS",
    "target": "ES2017",
    "baseUrl": "./",
    "outDir": "./dist",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strictPropertyInitialization": false,
    "forceConsistentCasingInFileNames": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "noImplicitAny": false
  },
  "exclude": ["node_modules"]
}
